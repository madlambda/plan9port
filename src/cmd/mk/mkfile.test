MKSHELL=$PLAN9/bin/rc
use-rc:V:
	for(i in a b c)
		echo $i

MKSHELL=/bin/sh
use-sh:V:
	for i in a b c
	do
		echo $i
	done

MKSHELL=$HOME/nashroot/bin/nash
use-nash:V:
	for i in ("a" "b" "c") {
		echo $i
	}

# complex nash syntax
use-nash2:V:
	var stdout, stderr, status <= ls /
	if $status != "0" {
		echo "error: " $stderr
		exit(1)
	}
	var paths <= split($stdout, "\n")
	for p in $paths {
		echo $p
	}


